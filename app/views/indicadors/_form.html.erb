<%= simple_form_for(@indicador) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tipus_indicador_id %>
    <%= f.input :producte_dapcons_id %>
    <%= f.input :etapa %>
    <%= f.input :valor %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
